		_s = Substitute ( _r ; "::" ; "¶" )
	] ;
	GetValue ( _s ; 1 )
)</Calculation></Value><Repetition><Calculation>1</Calculation></Repetition><Name>$table</Name></Step><Step enable="True" id="141" name="Set Variable"><Value><Calculation>//	formule évitant le codage en dur du nom de la rubrique
Let (
	[
		_r = GetFieldName ( zV::preferences ) ;
		_s = Substitute ( _r ; "::" ; "¶" )
	] ;
	GetValue ( _s ; 2 )
)</Calculation></Value><Repetition><Calculation>1</Calculation></Repetition><Name>$field</Name></Step><Step enable="True" id="89" name="# (comment)"/><Step enable="True" id="89" name="# (comment)"><Text>	------------------------------  TRAITEMENT</Text></Step><Step enable="True" id="89" name="# (comment)"><Text> 	Préparation de la réponse au format JSON</Text></Step><Step enable="True" id="141" name="Set Variable"><Value><Calculation>JSONSetElement ( "" ;
	[ "layout" ; $layout ; JSONString ] ;
	[ "table" ; $table ; JSONString ] ;
	[ "field" ; $table &amp; "::" &amp; $field ; JSONString ]
)</Calculation></Value><Repetition><Calculation>1</Calculation></Repetition><Name>$json</Name></Step><Step enable="True" id="89" name="# (comment)"/><Step enable="True" id="89" name="# (comment)"><Text>	------------------------------  FIN DE LA PROCÉDURE</Text></Step><Step enable="True" id="103" name="Exit Script"><Calculation>$json</Calculation></Step><Step enable="True" id="89" name="# (comment)"/><Step enable="True" id="89" name="# (comment)"><Text>/** 
